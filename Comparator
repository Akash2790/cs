using System;



public class Comparator
{
    public bool compare(int a,int b)
    {
        if(a==b)
        {
            return true;
        }
        return false;
    }
    public bool compare(string a,string b)
    {
        if(a==b)
        {
            return true;
        }
        return false;
    }
    public bool compare(int[] a,int[] b)
    {
        int al=a.Length;
        int bl=b.Length;
        if(al!=bl)
        {
            return false;
        }
        for(int i=0;i<al;i++)
        {
            if(a[i]!=b[i])
            {
               return false;
            }
        }
        return true;
        
    }
}
